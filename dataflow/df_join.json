{
	"name": "df_join",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "movies_ds1",
						"type": "DatasetReference"
					},
					"name": "movies1"
				},
				{
					"dataset": {
						"referenceName": "moviesdb_ds",
						"type": "DatasetReference"
					},
					"name": "moviesdb"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "moviesOpCSV",
						"type": "DatasetReference"
					},
					"name": "MoviesOP",
					"rejectedDataLinkedService": {
						"referenceName": "LS_BLOB_Learning777",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "moviesclean"
				},
				{
					"name": "joinmovies"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> movies1",
				"source(output(",
				"          movie as string,",
				"          title as string,",
				"          genres as string,",
				"          year as string,",
				"          Rating as string,",
				"          RottenTomato as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> moviesdb",
				"movies1 derive(titlenew = trim(toString(left(title,length(title)-6)))) ~> moviesclean",
				"moviesclean, moviesdb join(titlenew == moviesdb@title,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinmovies",
				"joinmovies select(mapColumn(",
				"          movieId,",
				"          title = movies1@title,",
				"          genres = movies1@genres,",
				"          movie,",
				"          genres = moviesdb@genres,",
				"          year,",
				"          Rating,",
				"          RottenTomato",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['MoviesOutPut'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> MoviesOP"
			]
		}
	}
}